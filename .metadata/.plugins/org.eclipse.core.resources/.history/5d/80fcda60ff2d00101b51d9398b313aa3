#ifndef INC_STM32F407XX_SPI_DRIVER_H_
#define INC_STM32F407XX_SPI_DRIVER_H_

typedef struct {
	uint32_t SPI_DeviceMode;
	uint32_t SPI_BusConfig;
	uint32_t SPI_SclkSpeed;
	uint32_t SPI_DFF;
	uint32_t SPI_CPOL;
	uint32_t SPI_CPHA;
	uint32_t SPI_SSM;
}SPI_Config_t;

#endif /* INC_STM32F407XX_SPI_DRIVER_H_ */
